{
  "title": "Suivi des \u00c9v\u00e9nements (Pub/Sub)",
  "uid": "events-dashboard",
  "schemaVersion": 27,
  "version": 1,
  "editable": true,
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "refresh": "10s",
  "panels": [
    {
      "type": "stat",
      "title": "\u00c9v\u00e9nements publi\u00e9s",
      "id": 1,
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 8,
        "h": 4
      },
      "datasource": "${DS_PROMETHEUS}",
      "targets": [
        {
          "expr": "sum(events_published_total)",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "\u00c9v\u00e9nements consomm\u00e9s",
      "id": 2,
      "gridPos": {
        "x": 8,
        "y": 0,
        "w": 8,
        "h": 4
      },
      "datasource": "${DS_PROMETHEUS}",
      "targets": [
        {
          "expr": "sum(event_consumed_total)",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "Commandes \u00e9mises",
      "id": 3,
      "gridPos": {
        "x": 16,
        "y": 0,
        "w": 8,
        "h": 4
      },
      "datasource": "${DS_PROMETHEUS}",
      "targets": [
        {
          "expr": "sum(commande_events_emitted_total)",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0
        },
        "overrides": []
      }
    },
    {
      "type": "graph",
      "title": "Latence des \u00e9v\u00e9nements (p95)",
      "id": 4,
      "gridPos": {
        "x": 0,
        "y": 4,
        "w": 24,
        "h": 8
      },
      "datasource": "${DS_PROMETHEUS}",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(event_latency_seconds_bucket[5m]))",
          "refId": "A"
        }
      ],
      "lines": true,
      "linewidth": 2,
      "nullPointMode": "null",
      "legend": {
        "show": true
      },
      "tooltip": {
        "shared": true,
        "sort": 0,
        "value_type": "individual"
      },
      "xaxis": {
        "show": true
      },
      "yaxes": [
        {
          "format": "s",
          "logBase": 1,
          "show": true
        },
        {
          "format": "short",
          "logBase": 1,
          "show": true
        }
      ]
    },
    {
      "type": "graph",
      "title": "\u00c9v\u00e9nements publi\u00e9s par type",
      "id": 5,
      "gridPos": {
        "x": 0,
        "y": 12,
        "w": 24,
        "h": 8
      },
      "datasource": "${DS_PROMETHEUS}",
      "targets": [
        {
          "expr": "sum(events_published_total) by (event_type, stream)",
          "refId": "A"
        }
      ],
      "lines": true,
      "linewidth": 2,
      "nullPointMode": "null",
      "legend": {
        "show": true
      },
      "tooltip": {
        "shared": true,
        "sort": 0,
        "value_type": "individual"
      },
      "xaxis": {
        "show": true
      },
      "yaxes": [
        {
          "format": "s",
          "logBase": 1,
          "show": true
        },
        {
          "format": "short",
          "logBase": 1,
          "show": true
        }
      ]
    },
    {
      "type": "graph",
      "title": "\u00c9v\u00e9nements consomm\u00e9s par type",
      "id": 6,
      "gridPos": {
        "x": 0,
        "y": 20,
        "w": 24,
        "h": 8
      },
      "datasource": "${DS_PROMETHEUS}",
      "targets": [
        {
          "expr": "sum(event_consumed_total) by (event_type, stream)",
          "refId": "A"
        }
      ],
      "lines": true,
      "linewidth": 2,
      "nullPointMode": "null",
      "legend": {
        "show": true
      },
      "tooltip": {
        "shared": true,
        "sort": 0,
        "value_type": "individual"
      },
      "xaxis": {
        "show": true
      },
      "yaxes": [
        {
          "format": "s",
          "logBase": 1,
          "show": true
        },
        {
          "format": "short",
          "logBase": 1,
          "show": true
        }
      ]
    },
    {
      "type": "graph",
      "title": "Latence par type d\u2019\u00e9v\u00e9nement (p95)",
      "id": 7,
      "gridPos": {
        "x": 0,
        "y": 28,
        "w": 24,
        "h": 8
      },
      "datasource": "${DS_PROMETHEUS}",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(event_latency_seconds_bucket[5m])) by (event_type)",
          "refId": "A"
        }
      ],
      "lines": true,
      "linewidth": 2,
      "nullPointMode": "null",
      "legend": {
        "show": true
      },
      "tooltip": {
        "shared": true,
        "sort": 0,
        "value_type": "individual"
      },
      "xaxis": {
        "show": true
      },
      "yaxes": [
        {
          "format": "s",
          "logBase": 1,
          "show": true
        },
        {
          "format": "short",
          "logBase": 1,
          "show": true
        }
      ]
    }
  ],
  "templating": {
    "list": []
  },
  "annotations": {
    "list": []
  },
  "timezone": "browser",
